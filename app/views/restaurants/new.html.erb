<h1>New Restaurant</h1>

<%= form_with model: @restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :neame %>
  <%= f.label :address %>
  <%= f.text_field :address %>
    <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
    <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.submit "Save" %>
<% end %>

<p><%= link_to "Back", restaurants_path %></p>
